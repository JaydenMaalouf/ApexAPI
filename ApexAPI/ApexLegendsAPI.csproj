<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>ApexAPI</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/JaydenMaalouf/ApexAPI/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/JaydenMaalouf/ApexAPI/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/JaydenMaalouf/ApexAPI/</RepositoryUrl>
    <PackageTags>Apex, Legends, API, Wrapper</PackageTags>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
    <Description>An unofficial .NET API Wrapper for the Fortnite API (https://apextab.com)</Description>
    <AssemblyVersion>1.0.2.0</AssemblyVersion>
    <FileVersion>1.0.2.0</FileVersion>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>

</Project>
